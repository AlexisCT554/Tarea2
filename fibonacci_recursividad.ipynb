      "source": [
        "def fibonacci( n ):\n",
        "  if n == 0:\n",
        "    return 0\n",
        "  elif n == 1:\n",
        "    return 1\n",
        "  else:\n",
        "    return ( fibonacci( n - 1) + fibonacci ( n-2 ))"
      ],
      "execution_count": 8,
      "outputs": []
